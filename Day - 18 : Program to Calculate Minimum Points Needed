#include <iostream>
using namespace std;

int main() {
    int T;
    cin >> T; // Read the number of test cases

    for (int i = 0; i < T; ++i) {
        int A, B;
        cin >> A >> B;

        // Calculate the minimum number of points needed to win
        int min_points = min(7 - A, 7 - B);

        cout << min_points << endl;
    }

    return 0;
}
