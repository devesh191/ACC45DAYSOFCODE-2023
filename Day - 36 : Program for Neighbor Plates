#include <stdio.h>

int main() {
    int T; // Number of test cases
    scanf("%d", &T);

    while (T--) {
        int X, Y; // Number of plates Chef prepared and plates each neighbor takes
        scanf("%d %d", &X, &Y);

        int max_neighbors = X / Y;
        printf("%d\n", max_neighbors);
    }

    return 0;
}
